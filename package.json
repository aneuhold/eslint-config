{
  "name": "@aneuhold/eslint-config",
  "version": "1.0.1",
  "description": "Main ESLint Configuration for personal projects",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "packageManager": "yarn@4.2.2",
  "scripts": {
    "pushpub": "npm run build && npm version patch && git push && npm publish --access public",
    "build": "tsc",
    "watch": "tsc -w",
    "upgrade:all": "yarn upgrade --latest",
    "lint": "yarn build && eslint"
  },
  "type": "commonjs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aneuhold/eslint-config.git"
  },
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "author": "Anton G. Neuhold Jr.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aneuhold/eslint-config/issues"
  },
  "homepage": "https://github.com/aneuhold/eslint-config#readme",
  "files": [
    "lib/**/*"
  ],
  "peerDependencies": {
    "eslint": ">= 9"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-svelte": "^2.39.0",
    "prettier-plugin-svelte": "^3.2.3",
    "typescript-eslint": "^7.8.0"
  },
  "devDependencies": {
    "@types/node": "^20.12.11",
    "eslint": "^9",
    "prettier": "^3.2.5",
    "svelte": "^4.2.17",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  }
}
