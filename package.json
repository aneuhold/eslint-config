{
  "name": "@aneuhold/eslint-config",
  "version": "1.3.15",
  "description": "Main ESLint Configuration for personal projects",
  "main": "./src/ts-lib-config.js",
  "packageManager": "pnpm@10.20.0",
  "scripts": {
    "dev": "nodemon -e js --exec \"local-npm publish\"",
    "unpub": "local-npm unpublish",
    "pushpub": "pnpm version patch && git push",
    "upgrade:all": "pnpm update --latest",
    "lint": "pnpm eslint"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aneuhold/eslint-config.git"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "prettier"
  ],
  "author": "Anton G. Neuhold Jr.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aneuhold/eslint-config/issues"
  },
  "homepage": "https://github.com/aneuhold/eslint-config#readme",
  "files": [
    "src/**/*"
  ],
  "peerDependencies": {
    "eslint": ">= 9.39.0",
    "prettier": ">= 3.6.2"
  },
  "dependencies": {
    "@eslint/js": "^9.39.0",
    "@next/eslint-plugin-next": "^16.0.1",
    "@stylistic/eslint-plugin-js": "^4.4.1",
    "@stylistic/eslint-plugin-ts": "^4.4.1",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "angular-eslint": "^20.5.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^61.1.11",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-svelte": "^3.13.0",
    "eslint-plugin-unused-imports": "^4.3.0",
    "globals": "^16.5.0",
    "prettier-plugin-svelte": "^3.4.0",
    "typescript-eslint": "^8.46.2"
  },
  "devDependencies": {
    "@aneuhold/local-npm-registry": "^0.2.14",
    "@types/node": "^24.9.2",
    "eslint": "^9.39.0",
    "prettier": "^3.6.2",
    "svelte": "^5.43.2",
    "typescript": "^5.9.3"
  }
